<Window x:Class="WpfApplicationDLL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="18,61,0,0" TextWrapping="Wrap" Text="a" VerticalAlignment="Top" Width="120" Name="textBoxA"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="174,61,0,0" TextWrapping="Wrap" Text="b" VerticalAlignment="Top" Width="120" Name="textBoxB"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="359,61,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Name="textBoxCalc"/>
        <Button Content="Calculate" HorizontalAlignment="Left" Margin="204,252,0,0" VerticalAlignment="Top" Width="75" Name="Calculate" Click="Calculate_Click"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="18,126,0,0" TextWrapping="Wrap" Text="float a" VerticalAlignment="Top" Width="120" Name="textBoxfA"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="174,126,0,0" TextWrapping="Wrap" Text="float b" VerticalAlignment="Top" Width="120" Name="textBoxfB"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="359,126,0,0" TextWrapping="Wrap" Text="Substracted" VerticalAlignment="Top" Width="120" Name="textBoxCalcSubstract"/>

    </Grid>
</Window>
